{% extends 'SonataAdminBundle::standard_layout.html.twig' %}
{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('//code.highcharts.com/4.1.8/highcharts.js') }}"></script>
    <script src="{{ asset('//code.highcharts.com/4.1.8/modules/exporting.js') }}"></script>

    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>

    <script src="{{ asset('build/app.js') }}"></script>

    <script type="text/javascript">
        {{ chart(chart1)}}
        {{ chart(chart2)}}
    </script>
{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('build/app.css') }}">
{% endblock %}
{% block content %}
    <div class="row">
        {# {{ sonata_block_render({ 'type': 'admin.block.service.statistic_chart' }, {
             'entity' : 'AppBundle:Statistic',
             'repository_method' : 'findAll',
             'title' : 'Statistic',
         }) }}#}
    </div>
    <div id="linechart_usd" style="min-width: 400px; height: 400px; margin-bottom: 20px"></div>
    <div id="linechart_btc" style="min-width: 400px; height: 400px; margin: 0 auto"></div>
{% endblock %}